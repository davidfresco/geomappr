{
    "manifest_version": 2,
    "name": "Geomappr",
    "version": "1.0",
    "description": "Saves your current geoguessr location to a map file",

    "permissions": [
        "activeTab",
        "storage"
    ],

    "browser_action": {
        "default_icon": "resources/globe2.png",
        "default_popup": "content/homepage.html"
    }
}
